build.im <- function(size) as.cimg(function(x,y) x+y,size,size)
liply(c(10,50,100),"x",build.im) %>% plot
liply(c(10,50,100),"y",build.im) %>% plot
build.im <- function(size) as.cimg(function(x,y) (x+y)/size,size,size)
liply(c(10,50,100),"y",build.im) %>% plot
parrots <- load.image(system.file('extdata/parrots.png',package='imager'))

ilply(parrots,"c",function(v) mean(v))
idply(parrots,"c",function(v) (v-mean(v))/sd(v)) %>% plot
iiply(parrots,"c",function(v) (v-mean(v))/sd(v)) %>% plot
